{
  "name": "sai-proxy-server",
  "version": "1.2.0",
  "description": "SAI Proxy Server - Secure webhook proxy for form submissions",
  "main": "proxy-server.js",
  "scripts": {
    "start": "node proxy-server.js",
    "health": "curl http://127.0.0.1:8003/api/health",
